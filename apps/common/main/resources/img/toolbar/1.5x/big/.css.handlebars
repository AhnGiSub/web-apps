{{#spritesheet}}
@media only screen {
    @media (-webkit-min-device-pixel-ratio: 1.5),
            (min-resolution: 1.5dppx), (min-resolution: 144dpi)
    {
        .x-huge .toolbar__icon {
            background-image: url(resources/{{{escaped_image}}});
            background-size: {{scaled width 1.5}}px auto;
        }
    }
}
{{/spritesheet}}
